@model IEnumerable<BeanSceneAppV1.Models.Area>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Area_Name)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => item.Area_Name)</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Area_Name">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Area_Name">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Area_Name">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
